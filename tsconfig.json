{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "dist",
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "target": "ES2019",
    "moduleResolution": "node",
    "declaration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
  },
  "exclude": [
    "node_modules"
  ]
}